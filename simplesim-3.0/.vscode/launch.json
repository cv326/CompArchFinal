{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "debug anagram",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/sim-bpred",
            "args": ["-bpred", "agree", "benchmarks/anagram.alpha", "words", "<benchmarks/anagram.in", ">OUT"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "lldb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for lldb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "build",
            "linux": {
                "MIMode": "gdb",
                "miDebuggerPath": "/usr/bin/gdb"
            },
            "windows": {
                "MIMode": "gdb",
                "miDebuggerPath": "/usr/bin/gdb.exe"
            },
            "osx": {
                "MIMode": "lldb",
                "miDebuggerPath": "/usr/local/bin/lldb-mi"
            }
        },
